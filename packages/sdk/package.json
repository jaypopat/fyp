{
	"name": "@zkfair/sdk",
	"version": "0.0.17",
	"type": "module",
	"main": "./index.ts",
	"types": "./index.ts",
	"files": [
		"dist",
		"*.ts",
		"schema"
	],
	"exports": {
		".": {
			"types": "./index.ts",
			"import": "./index.ts",
			"default": "./index.ts"
		},
		"./schema": {
			"types": "./schema/index.ts",
			"import": "./schema/index.ts",
			"default": "./schema/index.ts"
		},
		"./browser": {
			"types": "./browser.ts",
			"import": "./browser.ts",
			"default": "./browser.ts"
		},
		"./model": {
			"types": "./model.ts",
			"import": "./model.ts",
			"default": "./model.ts"
		},
		"./client": {
			"types": "./client.ts",
			"import": "./client.ts",
			"default": "./client.ts"
		},
		"./merkle": {
			"types": "./merkle.ts",
			"import": "./merkle.ts",
			"default": "./merkle.ts"
		},
		"./hash": {
			"types": "./hash.ts",
			"import": "./hash.ts",
			"default": "./hash.ts"
		},
		"./utils": {
			"types": "./utils.ts",
			"import": "./utils.ts",
			"default": "./utils.ts"
		}
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"prepublishOnly": "bun run build",
		"release:patch": "bun version patch && bun publish",
		"release:minor": "bun version minor && bun publish",
		"release:major": "bun version major && bun publish"
	},

	"publishConfig": {
		"access": "public",
		"main": "./dist/index.mjs",
		"types": "./dist/index.d.mts",
		"files": [
			"dist"
		],
		"exports": {
			".": {
				"types": "./dist/index.d.mts",
				"import": "./dist/index.mjs"
			},
			"./schema": {
				"types": "./dist/schema/index.d.mts",
				"import": "./dist/schema/index.mjs"
			},
			"./browser": {
				"types": "./dist/browser.d.mts",
				"import": "./dist/browser.mjs"
			},
			"./model": {
				"types": "./dist/model.d.mts",
				"import": "./dist/model.mjs"
			},
			"./client": {
				"types": "./dist/client.d.mts",
				"import": "./dist/client.mjs"
			},
			"./merkle": {
				"types": "./dist/merkle.d.mts",
				"import": "./dist/merkle.mjs"
			},
			"./hash": {
				"types": "./dist/hash.d.mts",
				"import": "./dist/hash.mjs"
			},
			"./utils": {
				"types": "./dist/utils.d.mts",
				"import": "./dist/utils.mjs"
			}
		}
	},
	"peerDependencies": {
		"drizzle-orm": "^0.44.7",
		"typescript": "^5.9.3"
	},
	"peerDependenciesMeta": {
		"drizzle-orm": {
			"optional": true
		}
	},
	"dependencies": {
		"@aztec/bb.js": "^2.0.3",
		"@noir-lang/acvm_js": "^1.0.0-beta.9-e7fb7f3.nightly",
		"@noir-lang/noir_js": "^1.0.0-beta.15",
		"@noir-lang/noirc_abi": "^1.0.0-beta.9-e7fb7f3.nightly",
		"papaparse": "^5.5.3",
		"poseidon-lite": "^0.3.0",
		"viem": "^2.43.3"
	},
	"devDependencies": {
		"@types/bun": "^1.3.5",
		"@types/circomlibjs": "^0.1.6",
		"@types/papaparse": "^5.5.2",
		"drizzle-orm": "^0.44.7",
		"@zkfair/contracts": "workspace:*",
		"@zkfair/zk-circuits": "workspace:*",
		"tsdown": "^0.18.3"
	}
}
