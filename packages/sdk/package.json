{
	"name": "@zkfair/sdk",
	"version": "0.0.17",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./schema": {
			"types": "./dist/schema/index.d.ts",
			"import": "./dist/schema/index.js"
		},
		"./browser": {
			"types": "./dist/browser.d.ts",
			"import": "./dist/browser.js"
		},
		"./model": {
			"types": "./dist/model.d.ts",
			"import": "./dist/model.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.js"
		},
		"./merkle": {
			"types": "./dist/merkle.d.ts",
			"import": "./dist/merkle.js"
		},
		"./hash": {
			"types": "./dist/hash.d.ts",
			"import": "./dist/hash.js"
		},
		"./utils": {
			"types": "./dist/utils.d.ts",
			"import": "./dist/utils.js"
		}
	},

	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"pub": "bun run build && bun publish"
	},

	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"drizzle-orm": "^0.38.0",
		"typescript": "^5"
	},
	"peerDependenciesMeta": {
		"drizzle-orm": {
			"optional": true
		}
	},
	"dependencies": {
		"@aztec/bb.js": "^2.0.3",
		"@noir-lang/acvm_js": "^1.0.0-beta.15",
		"@noir-lang/noir_js": "^1.0.0-beta.11",
		"@noir-lang/noirc_abi": "^1.0.0-beta.15",
		"papaparse": "^5.5.3",
		"poseidon-lite": "^0.3.0",
		"viem": "^2.38.3"
	},
	"devDependencies": {
		"@types/bun": "^1.3.0",
		"@types/circomlibjs": "^0.1.6",
		"@types/papaparse": "^5.3.16",
		"drizzle-orm": "^0.38.0",
		"@zkfair/contracts": "workspace:*",
		"@zkfair/zk-circuits": "workspace:*",
		"tsup": "^8.5.1"
	}
}
